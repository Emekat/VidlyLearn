@model VidlyLearn.ViewModels.CustomerVM
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>MembershipType: @Model.MembershipType.Name</li>
    @if (Model.BirthDate != null )
    {
        <li>Birthdate: @Model.BirthDate</li>
    }
</ul>

